{
  "name": "boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@firebase/app-types": "^0.3.2",
    "antd": "^3.6.2",
    "babel-plugin-import": "^1.8.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "firebase": "^5.9.2",
    "firebase-tools": "^3.19.3",
    "history": "^4.7.2",
    "immutable": "^3.7.4",
    "node-sass-chokidar": "^1.3.0",
    "normalize.css": "^8.0.0",
    "parcel-bundler": "^1.9.1",
    "prettier": "^1.13.5",
    "react": "^16.3.0",
    "react-app-rewire-less": "^2.1.1",
    "react-app-rewired": "^1.5.2",
    "react-dom": "^16.3.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scroll-parallax": "^2.0.4",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0",
    "webpack": "^3.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.7.0",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.2",
    "npm-run-all": "^4.1.5",
    "pretty-quick": "^1.6.0",
    "react-scripts": "1.1.1"
  },
  "scripts": {
    "build:dev": "npm-run-all sass:build --parallel start:dev sass:watch",
    "build:prod": "npm-run-all sass:build start:prod",
    "build:test": "npm-run-all sass:build --parallel start:test sass:watch",
    "start:dev": "REACT_APP_STAGE=development react-app-rewired start",
    "start:prod": "REACT_APP_STAGE=production react-app-rewired build",
    "start:test": "REACT_APP_STAGE=testing react-app-rewired test",
    "sass:watch": "node-sass-chokidar --watch src/ -o src/ --source-map true --output-style=compressed --include-path src/",
    "sass:build": "node-sass-chokidar src/ -o src/ --source-map true --output-style=compressed --include-path src/",
    "eject": "react-scripts eject",
    "precommit": "pretty-quick --staged"
  }
}
